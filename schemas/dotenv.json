{
    "type": "object",
    "$id": "schema:dotenv",
    "required": [
        "DB_URL"
    ],
    "properties": {
        "NODE_ENV": {
            "type": "string",
            "default": "development"
        },
        "PORT": {
            "type": "integer",
            "default": 3000
        },
        "DB_URL": {
            "type": "string"
        }
    }
}